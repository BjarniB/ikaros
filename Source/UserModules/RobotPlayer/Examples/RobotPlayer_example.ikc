<?xml version="1.0"?>

<group title="RobotToPos Example">

	<module
        class = "Dynamixel"
        name = "Dynamixel"
	device = "/dev/ttyUSB0"
        servo_id = "1 2 3"
        index_mode = "indirect"
        printf_info ="none"
        use_feedback ="true"
    	baud_rate = "1000000"
    	angle_unit = "degrees"
    	/>


  
  
	<module
		class		=	"FunctionGenerator"
		name		=	"Speed"
		type		=	"sin"
		frequency	=	"0"
		offset     	=	"0.5"
		amplitude	=	"0"
		shift		=	"0"
	/>
		<module
		class		=	"FunctionGenerator"
		name		=	"Torque"
		type		=	"sin"
		frequency	=	"0.13"
		offset      	=	"0.35"
		amplitude	=	"0.15"
		shift		=	"100"
	/>		
		<module
		class = "Constant"
		name = "TorqueEnable"
		outputsize = "1"
		data = "1"
		/>
		
		<module
		class = "Constant"
		name = "RecordingState"
		outputsize = "1"
		data = "0"
		/>
		<module
			class="RobotPlayer"
			name="RobotPlayer"
			maxTicks = "10000"
		/>


<connection sourcemodule  = "TorqueEnable" source  = "OUTPUT" targetmodule	 = "Dynamixel" target  = "TORQUE_ENABLE" />
<connection sourcemodule  = "TorqueEnable" source  = "OUTPUT" targetmodule	 = "Dynamixel" target  = "TORQUE_ENABLE" />
<connection sourcemodule  = "TorqueEnable" source  = "OUTPUT" targetmodule	 = "Dynamixel" target  = "TORQUE_ENABLE" />

<connection sourcemodule  = "Torque" source  = "OUTPUT" targetmodule	 = "Dynamixel" target  = "TORQUE_LIMIT" />
<connection sourcemodule  = "Torque" source  = "OUTPUT" targetmodule	 = "Dynamixel" target  = "TORQUE_LIMIT" />
<connection sourcemodule  = "Torque" source  = "OUTPUT" targetmodule	 = "Dynamixel" target  = "TORQUE_LIMIT" />
		
<!-- Order is important -->

